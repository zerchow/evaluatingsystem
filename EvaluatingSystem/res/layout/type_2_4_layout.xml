<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView 
        android:id="@+id/type_2_4_doctor"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_eval_date"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_eval_start"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_eval_end"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_eval_millisecond"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_correct"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_error"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_test_start"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_test_end"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView 
        android:id="@+id/type_2_4_test_millisecond"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"/>
</LinearLayout>